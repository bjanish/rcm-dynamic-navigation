{
    "name": "reliv/rcm",
    "description": "Reliv Content Management System",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "reliv",
        "content",
        "manager",
        "module",
        "zf2"
    ],
    "homepage": "http://www.reliv.com/",
    "authors": [
        {
            "name": "Westin Shafer",
            "email": "wshafer@relivinc.com",
            "homepage": "http://www.reliv.com"
        },
        {
            "name": "Rod McNew",
            "email": "rmcnew@relivinc.com",
            "homepage": "http://www.reliv.com/"
        },
        {
            "name": "Brian Janish",
            "email": "bjanish@relivinc.com",
            "homepage": "http://www.reliv.com/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/reliv/RcmUser.git"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "doctrine/doctrine-orm-module": "0.*",
        "zendframework/zendframework": "2.*",
        "rwoverdijk/assetmanager": "1.*",
        "reliv/rcm-user": "0.1.*",
        "fxp/composer-asset-plugin": "~1.0",
        "bower-asset/angular":"1.2.20",
        "bower-asset/jquery": "1.10.2",
        "bower-asset/bootstrap": "3.3.4",
        "bower-asset/bootbox": "4.3.0",
        "bower-asset/bootstrap-tabcollapse": "0.2.4",
        "bower-asset/html5shiv": "3.7.2",
        "bower-asset/es5-shim": "4.1.1",
        "bower-asset/respond": "1.4.2",
        "bower-asset/rcm-js-lib": "0.*",
        "bower-asset/rcm-loading": "0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "1.5.*"
    },
    "autoload": {
        "psr-4": {
            "Rcm\\": "src/"
        }
    }
}
